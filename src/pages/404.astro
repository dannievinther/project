---
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
---
<html lang="da">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Exercssises</title>
    <link rel="icon" type="image/svg+xml" href="/favicon.svg">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto+Mono:wght@500&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/style/reset.css" />
    <link rel="stylesheet" href="/style/prism.css" />
    <link rel="stylesheet" href="/style/custom.css" />
    <style>
        main {
            place-items: center;
        }
    </style>
</head>
<body>

    <header class="main-header">
      <h1 aria-label="404 Page not found!">
        4<span class="stroke-text">0</span>4
      </h1>
    </header>
    <main class="place-items">
      <section>
        <article>
          <h2>Page was not found</h2>
          <p>Try again, or go <a href="./">back to the exercises</a>.</p>
        </article>
      </section>
    </main>

    <Footer />

    <script src="/script/bliss.js"></script>
    <script src="/script/prism.js"></script>
    <script src="/script/prism-live.js"></script>
    <script src="/script/prism-live-markup.js"></script>
    <script src="/script/prism-live-css.js"></script>
    <script type="module" src="/script/custom.js"></script>
</body>
</html>
