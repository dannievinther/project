---
import BaseLayout from '../layouts/Base.astro'
import Exercise from "../components/Exercise.astro";
const localData = Astro.fetchContent('../exercises/flex/*.md');
---
<!DOCTYPE html>
<BaseLayout title="- Flex">
    {localData.map((ex, i, all) => (
        <Exercise dataSuffix="flex" data={ex} num={i+1} all={all.length} />
    ))}
</BaseLayout>